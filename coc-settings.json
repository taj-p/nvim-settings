{
  "coc.preferences.formatOnSaveFiletypes": [
    "svelte",
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "markdown",
    "md"
  ],
  "eslint.filetypes": [
    "svelte",
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  "coc.preferences.diagnostic.virtualText": true,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "svelte.enable-ts-plugin": true
}
